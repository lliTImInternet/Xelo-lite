      "scripts": {
        "scale": "0.9375",
        "initialize": [
          "variable.is_holding_right = 0.0;",
          "variable.render = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;",
          "variable.helmet_enchanted = 0.0;",
          "variable.chestplate_enchanted = 0.0;",
          "variable.leggings_enchanted = 0.0;",
          "variable.boots_enchanted = 0.0;",
          "variable.is_walking_north = 0.0;",
          "variable.is_walking_west = 0.0;",
          "variable.is_walking_south = 0.0;",
          "variable.is_walking_east = 0.0;",
          "variable.is_facing_north = 0.0;",
          "variable.is_facing_west = 0.0;",
          "variable.is_facing_south = 0.0;",
          "variable.is_facing_east = 0.0;",
          "variable.temp_w = 0.0;",
          "variable.temp_a = 0.0;",
          "variable.temp_s = 0.0;",
          "variable.temp_d = 0.0;",
          "variable.is_w = 0.0;",
          "variable.is_a = 0.0;",
          "variable.is_s = 0.0;",
          "variable.is_d = 0.0;",
          "variable.keystroke = 0.0;",
          "variable.speed = 0.0;"
        ],
        "pre_animation": [
          "temp.chyves_rotation = temp.chyves_rotation ?? 0;",
          "variable.speed = query.is_moving ? (query.ground_speed - query.vertical_speed) : 0;",
          "variable.keystroke = Math.mod(360 + Math.mod(Math.atan2(query.position_delta(0), query.position_delta(2)) + query.head_y_rotation(0), 360), 360);",
          "variable.is_walking_north = variable.speed > 0.05 && ((variable.keystroke >= 23) && (variable.keystroke <= 157));",
          "variable.is_walking_west = variable.speed > 0.05 && ((variable.keystroke >= 112) && (variable.keystroke <= 248));",
          "variable.is_walking_south = variable.speed > 0.05 && ((variable.keystroke >= 203) && (variable.keystroke <= 337));",
          "variable.is_walking_east = variable.speed > 0.05 && (((variable.keystroke >= 293) && (variable.keystroke <= 360)) || ((variable.keystroke >= 0) && (variable.keystroke <= 67)));",
          "variable.is_facing_north = variable.speed > 0.05 && ((temp.chyves_rotation >= 23) && (temp.chyves_rotation <= 157));",
          "variable.is_facing_east = variable.speed > 0.05 && ((temp.chyves_rotation >= 112) && (temp.chyves_rotation <= 248));",
          "variable.is_facing_south = variable.speed > 0.05 && ((temp.chyves_rotation >= 203) && (temp.chyves_rotation <= 337));",
          "variable.is_facing_west = variable.speed > 0.05 && (((temp.chyves_rotation >= 293) && (temp.chyves_rotation <= 360)) || ((temp.chyves_rotation >= 0) && (temp.chyves_rotation <= 67)));",
          "variable.temp_w = (variable.is_walking_north && variable.is_facing_north) || (variable.is_walking_west && variable.is_facing_west) || (variable.is_walking_south && variable.is_facing_south) || (variable.is_walking_east && variable.is_facing_east);",
          "variable.temp_a = (variable.is_walking_west && variable.is_facing_north) || (variable.is_walking_south && variable.is_facing_west) || (variable.is_walking_east && variable.is_facing_south) || (variable.is_walking_north && variable.is_facing_east);",
          "variable.temp_s = (variable.is_walking_north && variable.is_facing_south) || (variable.is_walking_south && variable.is_facing_north) || (variable.is_walking_west && variable.is_facing_east) || (variable.is_walking_east && variable.is_facing_west);",
          "variable.temp_d = (variable.is_walking_east && variable.is_facing_north) || (variable.is_walking_north && variable.is_facing_west) || (variable.is_walking_west && variable.is_facing_south) || (variable.is_walking_south && variable.is_facing_east);",
          "variable.is_w = (variable.temp_w && variable.temp_s) ? 0 : variable.temp_w;",
          "variable.is_a = (variable.temp_a && variable.temp_d) ? 0 : variable.temp_a;",
          "variable.is_s = (variable.temp_s && variable.temp_w) ? 0 : variable.temp_s;",
          "variable.is_d = (variable.temp_d && variable.temp_a) ? 0 : variable.temp_d;",
          "variable.helmet_enchanted = query.armor_material_slot(0) == 1 || query.armor_material_slot(0) == 3;",
          "variable.chestplate_enchanted = query.armor_material_slot(1) == 1 || query.armor_material_slot(1) == 3;",
          "variable.leggings_enchanted = query.armor_material_slot(2) == 1 || query.armor_material_slot(2) == 3;",
          "variable.boots_enchanted = query.armor_material_slot(3) == 1 || query.armor_material_slot(3) == 3;",
          "variable.helmet_layer_visible = 1.0;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
          "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.first_person_rotation_factor = math.sin((1 - variable.attack_time) * 180.0);",
          "variable.hand_bob = query.life_time < 0.01 ? 0.0 : variable.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;",
          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0.0, 1.0);",
          "variable.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;"
        ],
        "animate": [
          "chyves",
          "root",
          "helmet",
          "chestplate",
          "leggings",
          "boots",
          "offhand_hud",
          {
            "render": "query.is_in_ui"
          }
        ]
