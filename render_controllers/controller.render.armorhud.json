        "controller.render.helmet": {
            "geometry": "Geometry.helmet",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "(query.armor_texture_slot(0) == 0 ? texture.leather_helmet : (query.armor_texture_slot(0) == 1 ? texture.chainmail_helmet : (query.armor_texture_slot(0) == 2 ? texture.iron_helmet :(query.armor_texture_slot(0) == 3 ? texture.diamond_helmet :(query.armor_texture_slot(0) == 4 ? texture.gold_helmet :(query.armor_texture_slot(0) == 6 ? texture.turtle_helmet :(query.armor_texture_slot(0) == 7 ? texture.netherite_helmet)))))))"
            ],
            "is_hurt_color": {
                "r": "variable.helmet_enchanted ? 0.4 : 0.0",
                "g": 0,
                "b": "variable.helmet_enchanted",
                "a": "variable.helmet_enchanted ? 0.05 * math.cos(query.life_time* 425.0) + 0.25 : 0.0"
            },
            "on_fire_color": {
                "r": "variable.helmet_enchanted ? 0.4 : 0.0",
                "g": 0,
                "b": "variable.helmet_enchanted",
                "a": "variable.helmet_enchanted ? 0.05 * math.cos(query.life_time* 425.0) + 0.25 : 0.0"
