                "b": "variable.helmet_enchanted",
                "a": "variable.helmet_enchanted ? 0.05 * math.cos(query.life_time* 425.0) + 0.25 : 0.0"
            },
            "light_color_multiplier": "variable.helmet_enchanted ? 0.05 * -math.cos(query.life_time * 500.0) + 1.5 : 1.15",
            "ignore_lighting": true
