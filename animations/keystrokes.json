{
    "format_version": "1.8.0",
    "animations": {
        "animation.chyves.keystrokes": {
            "loop": true,
            "bones": {
                "chyves": {
                    "position": [
                        0,
                        "query.is_swimming ? -11 : 0",
                        0
                    ],
                    "rotation": [
                        "(query.is_gliding ? -query.body_x_rotation + -90)",
                        "(query.is_gliding ? -query.body_y_rotation) + (query.is_riding ? query.target_y_rotation)",
                        0
                    ]
                }
            }
        }
    }
}